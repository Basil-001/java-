//A five-question numeral product test??
		int score=0;
		int n=5;
		
		for(int i=0;i<n;i++)
		{
			int n1=(int)(Math.random()*9);
			int n2=(int)(Math.random()*9);
			
			System.out.print(n1+" + "+n2+" = ");
			int answer=input.nextInt();
			
			if(answer == (n1+n2))
				score++;
			
		}
		System.out.println("Your answer is : "+score+" of "+n);
